//Example AnalogRead() code

int x_input = A0; 
int y_input = A1;
int z_input = A2;

int val0 = 0;  // variable to store the value read
int val1 = 1;
int val2 = 2;

void setup() {
  Serial.begin(9600);           //  setup serial
}

void loop() {
  val0 = analogRead(A0);  // read the input pin
  Serial.println(val0);          // debug value
  
  val1 = analogRead(A1);
  Serial.println(val1);
  
  val2 = analogRead(A2);
  Serial.println(val2);
  
}
